<table border="1">
  <tr>
    <th>ISBN code</th><th>title</th><th>price</th>
    <th>publisher</th><th>publish date</th><th>CD-ROM</th>
  </tr>

  <% @books.each do |book| %>
    <tr>
      <td><%= book.isbn %> </td>
      <td><%= book.title %> </td>
      <td><%= book.price %> </td>
      <td><%= book.publish %> </td>
      <td><%= book.published %> </td>
      <td><%= book.cd %> </td>
    </tr>

    <% end %>
</table>
